{
	"$schema": "https://turbo.build/schema.json",
	"tasks": {
		"build": {
			"dependsOn": ["^build"],
			"outputs": [
				"dist/**",
				".next/**",
				"public/build/**",
				"build/**",
				"app/styles/tailwind.css",
				".cache"
			]
		},
		"typecheck": {
			"dependsOn": ["^build"],
			"outputs": []
		}
	}
}
